@mixin-definition card {
  border-radius: 2px;
  trait: lowElevation from 'box-shadow';
  trait: white from 'background-color';
  line-height: 24px;
  margin-top: 48px;
}

@mixin-definition richMedia {
  /* Should be 16:9 */
}

@mixin-definition primaryTitle {
  margin: 24px 16px 16px 16px;
  font-size: 24px;
  line-height: 32px;
}

@mixin-definition actions {
  margin-top: 16px;
  margin-bottom: 16px;
}

@mixin-definition supportingText {
  margin: 16px 16px 24px 16px;
}